digraph Plan {
	graph [dpi = 100];

	json projectfiles database

	gpr2ld json2gpr querytool

	startup [label = "startup code"]
	ldscript [label = "linker script"]

	subgraph cluster_tool {
		penwidth=0;
		labelloc="t";

		{rank = same; querytool; json2gpr; gpr2ld;}
		{rank = same; database; projectfiles; json;}

		database -> querytool
		querytool -> database
		querytool -> json
		json -> json2gpr
		json2gpr -> projectfiles
		projectfiles -> gpr2ld

		gpr2ld -> startup
		gpr2ld -> ldscript

	}
	projectfiles [label = "Fichiers projets"]
	database [label = "Base de donn√©es"]
	querytool [shape = box]
	gpr2ld [shape = box]
	json2gpr [shape = box]
	json [label = "JSON"]
}
