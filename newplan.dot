digraph Plan {
	graph [dpi = 100];

	database;

	subgraph cluster_tool {
		style=filled;
    		fillcolor=red;
		penwidth=0;
		label="Toolchain"
		labelloc="t";
		{rank = same; gpr2ld; json2gpr; querytool;}
		json2gpr -> gpr2ld
		querytool -> json2gpr
	}

	subgraph cluster_compilation {
		style=filled;
    		fillcolor=grey;
		penwidth=0;
		label="Compilation"
		labelloc="t";
		{rank = same; program; files;}
		program -> gcc
		files -> gcc
		gcc -> exec
		exec -> dev [label=" Flashing executable"]
	}

	gcc [shape=box; style=dotted; label="GCC"];
	exec [shape=box; label="ExÃ©cutable"];
	program [label="Programme\nutilisateur"];
	files [label="LDSCRIPT"];
	dev [penwidth=0 width=1 height=1 fixedsize=true image="embedded.png" label=""]

	querytool -> database [constraint = false]
	database -> querytool [constraint = false]
	gpr2ld -> files [constraint = false]
}
